          },
          "bk2_name": {
            "type": "string",
            "description": "Второй букмекер",
            "example": "fonbet"
          },
          "bk1_bet_type": {
            "description": "Тип первой ставки",
            "$ref": "#/components/schemas/BetType"
          },
          "bk2_bet_type": {
            "description": "Тип второй ставки",
