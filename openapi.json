          },
          "sport": {
            "type": "string",
            "description": "Спорт",
            "example": "tennis"
          },
          "blocked": {
            "type": "boolean",
            "description": "Индикатор блокировки одной из ставок"
          },
          "bk1_game": {
            "type": "string",
            "description": "Название первого события"
          },
          "bk2_game": {
            "type": "string",
            "description": "Название второго события"
          },
          "bk1_bet": {
            "type": "string",
            "description": "Первая ставка"
          },
          "bk2_bet": {
            "type": "string",
            "description": "Вторая ставка"
          },
          "bk1_alt_bet": {
            "type": "string",
            "nullable": true,
            "description": "Альтернативная первая ставка"
          },
          "bk2_alt_bet": {
            "type": "string",
