  "paths": {
    "/api/sports": {
      "get": {
        "tags": [
          "Вспомогательное"
        ],
        "summary": "Виды спорта",
        "description": "Возвращает список доступных видов спорта с описанием",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Sport"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          }
        }
      }
    },
    "/api/bookmakers": {
      "get": {
        "tags": [
          "Вспомогательное"
        ],
        "summary": "Букмекеры",
        "description": "Возвращает список доступных букмекеров с описанием. В список войдут только те букмекеры, которые были запрошены/оплачены в Вашем тарифе.",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Bookmaker"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          }
        }
      }
    },
    "/api/forks": {
      "get": {
        "tags": [
          "LIVE-Вилки"
        ],
        "description": "Возвращает список LIVE-вилок по заданным параметрам",
        "summary": "Получить список LIVE-вилок",
        "parameters": [
          {
            "name": "bet_types",
            "description": "Тип ставок в вилке",
            "in": "query",
            "style": "form",
            "explode": false,
            "example": "HALF_WIN,WIN",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/BetType"
              }
            }
          },
          {
            "name": "sport",
            "description": "Спорт",
            "in": "query",
            "style": "form",
            "explode": false,
            "example": "tennis,soccer",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "event_id",
            "description": "Получение вилок только по указанному id события",
            "in": "query",
            "schema": {
              "type": "integer"
            },
            "example": 181993
          },
          {
            "name": "bk2_name",
            "description": "Список букмекеров, участвующих в вилке",
            "in": "query",
            "required": true,
            "style": "form",
            "explode": false,
            "example": "fonbet,olimp,bet365",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "bk_name",
            "description": "Букмекер, который обязательно должен участвовать в вилке",
            "in": "query",
            "example": "fonbet",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "min_cf",
            "description": "Минимальный коэффициент",
            "in": "query",
            "example": 1.1,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "max_cf",
            "description": "Максимальный коэффициент",
            "in": "query",
            "example": 3.5,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "min_fi",
            "description": "Минимальный % вилки",
            "in": "query",
            "example": -1,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "max_fi",
            "description": "Максимальный % вилки",
            "in": "query",
            "example": 10,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "is_middles",
            "description": "Коридоры в выдачи. -1 - без коридоров. 0 - вилки и коридоры. 1 - только коридоры",
            "in": "query",
            "schema": {
              "type": "integer",
              "default": 0,
              "enum": [
                -1,
                0,
                1
              ]
            }
          },
          {
            "name": "alive_sec",
            "description": "Минимальное время существования вилки",
            "in": "query",
            "schema": {
              "type": "number",
              "default": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Возвращает массив Fork",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Fork"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          },
          "422": {
            "$ref": "#/components/responses/MissingParameterError"
          }
        }
      }
    },
    "/api/fork": {
      "get": {
        "tags": [
          "LIVE-Вилки"
        ],
        "summary": "Получить детали вилки",
        "description": "Запрос для получения подробных деталей по отдельной вилке (по её ключу)",
        "parameters": [
          {
            "name": "key",
            "description": "Идентификатор вилки",
            "required": true,
            "in": "query",
            "example": "182128_6788852932bc1c34fc3f901f1dff25e2",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Возвращает Fork",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Fork"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          },
          "404": {
            "description": "Вилка не найдена",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "422": {
            "$ref": "#/components/responses/MissingParameterError"
          }
        }
      }
    },
    "/api/bk_events": {
      "get": {
        "tags": [
          "LIVE-События"
        ],
        "summary": "Получить список всех активных событий",
        "description": "Запрос для получения всех активных событий",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/BkEvent"
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          }
        }
      }
    },
    "/api/bk_event": {
      "get": {
        "tags": [
          "LIVE-События"
        ],
        "summary": "Поиск события на определенном букмекере",
        "description": "Запрос для поиска определенного события по id или названию",
        "parameters": [
          {
            "name": "name",
            "description": "Название события",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "example": "Manchester United vs Chelsea"
          },
          {
            "name": "id",
            "description": "id события",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "example": "1143492113045229"
          },
          {
            "name": "full",
            "description": "Включает markets в ответ",
            "in": "query",
            "schema": {
              "type": "boolean",
              "default": true
            }
          },
          {
            "name": "bet",
            "description": "Оставляет в markets только указанную ставку. Работает только при full=true",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "example": "WIN__P1"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BkEvent"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          },
          "404": {
            "description": "Событие не найдено",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "422": {
            "$ref": "#/components/responses/MissingParameterError"
          }
        }
      }
    },
    "/api/event": {
      "get": {
        "tags": [
          "LIVE-События"
        ],
        "summary": "Получить группу связанных событий",
        "description": "Запрос для получения всех событий с разных букмекеров, которые объединены в одну группу",
        "parameters": [
          {
            "name": "token",
            "required": true,
            "description": "Ключ пользователя",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "example": "dasfhd71316t3sdf03271g2mfdsoe712"
          },
          {
            "name": "id",
            "required": true,
            "description": "id события",
            "in": "query",
            "schema": {
              "type": "integer"
            },
            "example": 182220
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GlobalEvent"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/UnauthorizedError"
          },
          "404": {
            "description": "Событие не найдено",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "422": {
            "$ref": "#/components/responses/MissingParameterError"
          }
        }
      }
    }
  },
